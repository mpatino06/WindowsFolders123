@using WindowsFolders123.Core.Models;
@model List<Folder>

<br />
<div class="row">
	<div id="divSubFolder">


		@if (Model.Count > 0)
		{
			<div class="col-md-12">
				<div class="box-solid">
					<div class="box-body table-responsive no-padding">

						<table class="table table-hover table-bordered" id="TableSubFolder">
							<thead>
								<tr>
									<th>
										Id
									</th>
									<th>
										Descripción
									</th>
								</tr>
							</thead>
							<tbody>
								@{
									foreach (var item in Model)
									{
										<tr>
											<td>@item.Id</td>
											<td>@item.PathFile</td>
											
										</tr>
									}
								}
							</tbody>
						</table>
					</div>
				</div>
			</div>

		}
		else
		{
			<span class="help-block"> No Existen datos para mostrar</span>
		}

	</div>


</div>